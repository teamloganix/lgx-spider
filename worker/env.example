# Redis
REDIS_URL=redis://localhost:6379/0
REDIS_QUEUE=stl_jobs
REDIS_DLQ=stl_jobs_dlq
REDIS_RESULT_PREFIX=stl:result:
REDIS_RESULT_TTL_SECONDS=86400

# Analyzer API
ANALYZER_URL=https://labs.loganix.com/ux-tracking/core/api/link_evaluation.php
ANALYZER_TIMEOUT_SECONDS=120

# Ahrefs API
AHREFS_API_KEY=
AHREFS_BASE_URL=https://api.ahrefs.com/v3

# SerpAPI
SERPAPI_KEY=
SERPAPI_BASE_URL=https://serpapi.com/search.json

# OpenRouter
OPENROUTER_API_KEY=
OPENROUTER_MODEL=anthropic/claude-3.5-sonnet
OPENROUTER_ENDPOINT=https://openrouter.ai/api/v1/chat/completions

# PostgreSQL
DATABASE_URL=postgresql://user:password@localhost:5432/dbname
PG_TABLE=stl_results

# Worker
JOB_MAX_RETRIES=3
JOB_VISIBILITY_TIMEOUT=0

# Logging
LOG_LEVEL=INFO
